{
  "tag": "room",
  "type": "room",
  "status": "init",
  "timeout": 600,
  "triggers": [
    {
      "trigger_tag": "room",
      "trigger_status": "api_hangup",
      "action": "terminate",
      "active": true
    },
    {
      "trigger_tag": "bridge_main",
      "trigger_status": "DESTROY",
      "action": "terminate",
      "active": true
    },
    {
      "trigger_tag": "client",
      "trigger_status": "StasisEnd",
      "action": "terminate",
      "active": true
    },
    {
      "trigger_tag": "specialist",
      "trigger_status": "StasisEnd",
      "action": "terminate",
      "active": true
    }
  ],
  "content": [
    {
      "type": "bridge",
      "tag": "bridge_main",
      "status": "init",
      "triggers": [
        {
          "trigger_tag": "room",
          "trigger_status": "ready",
          "action": "start",
          "active": true
        },
        {
          "trigger_tag": "specialist",
          "trigger_status": "StasisEnd",
          "action": "terminate",
          "active": true
        },
        {
          "trigger_tag": "client",
          "trigger_status": "StasisEnd",
          "action": "terminate",
          "active": true
        }
      ],
      "content": [
        {
          "type": "chan_outbound",
          "tag": "specialist",
          "status": "init",
          "params": {
            "dial_option_name": "intphone"
          },
          "triggers": [
            {
              "trigger_tag": "bridge_main",
              "trigger_status": "BridgeCreated",
              "action": "start",
              "active": true
            }
          ]
        },
        {
          "type": "chan_outbound",
          "tag": "client",
          "status": "init",
          "params": {
            "dial_option_name": "extphone"
          },
          "triggers": [
            {
              "trigger_tag": "specialist",
              "trigger_status": "Dial#ANSWER",
              "action": "start",
              "active": true
            }
          ]
        }
      ]
    },
    {
      "type": "bridge",
      "tag": "bridge_snoop_specialist",
      "status": "init",
      "triggers": [
        {
          "trigger_tag": "specialist",
          "trigger_status": "StasisStart",
          "action": "start",
          "active": true
        },
        {
          "trigger_tag": "specialist",
          "trigger_status": "StasisEnd",
          "action": "terminate",
          "active": true
        }
      ],
      "content": [
        {
          "type": "chan_emedia",
          "tag": "emedia_specialist",
          "status": "init",
          "params": {
            "external_host": "127.0.0.1:9999"
          },
          "triggers": [
            {
              "trigger_tag": "bridge_snoop_specialist",
              "trigger_status": "BridgeCreated",
              "action": "start",
              "active": true
            }
          ]
        },
        {
          "type": "chan_snoop",
          "tag": "snoop_specialist",
          "status": "init",
          "params": {
            "target_chan_tag": "specialist"
          },
          "triggers": [
            {
              "trigger_tag": "emedia_specialist",
              "trigger_status": "StasisStart",
              "action": "start",
              "active": true
            }
          ]
        }
      ]
    },
    {
      "type": "bridge",
      "tag": "bridge_snoop_client",
      "status": "init",
      "triggers": [
        {
          "trigger_tag": "client",
          "trigger_status": "StasisStart",
          "action": "start",
          "active": true
        },
        {
          "trigger_tag": "client",
          "trigger_status": "StasisEnd",
          "action": "terminate",
          "active": true
        }
      ],
      "content": [
        {
          "type": "chan_snoop",
          "tag": "snoop_client",
          "status": "init",
          "params": {
            "target_chan_tag": "client"
          },
          "triggers": [
            {
              "trigger_tag": "bridge_snoop_specialist",
              "trigger_status": "BridgeCreated",
              "action": "start",
              "active": true
            }
          ]
        },
        {
          "type": "chan_emedia",
          "tag": "emedia_client",
          "status": "init",
          "params": {
            "external_host": "127.0.0.1:9999"
          },
          "triggers": [
            {
              "trigger_tag": "snoop_client",
              "trigger_status": "StasisStart",
              "action": "start",
              "active": true
            }
          ]
        }
      ]
    }
  ]
}