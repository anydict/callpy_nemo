              "trigger_status": "ChannelHangupRequest",
              "action": "func",
              "func": "get_sip_and_q850",
              "active": true
            },
            {
              "trigger_tag": "client",
              "trigger_status": "StasisEnd",
              "action": "func",
              "func": "get_sip_and_q850",
              "active": true
            }
          ],
          "content": [
            {
              "type": "clip",
              "tag": "ivr_clip",
              "status": "init",
              "params": {
                "wait_before": 1,
                "wait_after": 6,
                "repeat": 1,
                "audio_name": "custom/ivrclip_redir1_noclip8"
              },
              "triggers": [
                {
                  "trigger_tag": "client",
                  "trigger_status": "Dial#ANSWER",
                  "action": "start",
                  "active": true
                },
                {
                  "trigger_tag": "clip_button_1",
                  "trigger_status": "init",
                  "action": "terminate",
                  "active": true
                },
                {
                  "trigger_tag": "clip_button_8",
                  "trigger_status": "init",
                  "action": "terminate",
                  "active": true
                },
                {
                  "trigger_tag": "ivr_clip",
                  "trigger_status": "PlaybackFinished",
                  "action": "func",
                  "func": "check_fully_playback",
                  "active": true
                }
              ]
            },
            {
              "type": "clip",
              "tag": "clip_button_1",
              "status": "init",
              "params": {
                "wait_before": 1,
                "wait_after": 1,
                "repeat": 1,
                "audio_name": "hello"
              },
              "triggers": [
                {
                  "trigger_tag": "client",
                  "trigger_status": "ChannelDtmfReceived#1",
                  "action": "start",
                  "active": true
                }
              ]
            },
            {
              "type": "clip",
              "tag": "clip_button_8",
              "status": "init",
              "params": {
                "wait_before": 1,
                "wait_after": 1,
                "repeat": 1,
                "audio_name": "goodbye"
              },
              "triggers": [
                {
                  "trigger_tag": "client",
                  "trigger_status": "ChannelDtmfReceived#8",
                  "action": "start",
                  "active": true
                }
              ]
            }
          ]
        },
        {
          "type": "chan_outbound",
          "tag": "specialist",
          "status": "init",
          "params": {
            "dial_option_name": "intphone"
          },
          "triggers": [
            {
              "trigger_tag": "clip_button_1",
              "trigger_status": "PlaybackFinished",
              "action": "start",
              "active": true
            }
          ]
        }
      ]
    },
    {
      "type": "bridge",
      "tag": "bridge_snoop_specialist",
      "status": "init",
      "triggers": [
        {
          "trigger_tag": "specialist",
          "trigger_status": "StasisStart",
