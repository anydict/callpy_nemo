        {
          "trigger_tag": "room",
          "trigger_status": "stop",
          "action": "terminate",
          "active": true
        }
      ],
      "content": [
        {
          "type": "chan_outbound",
          "tag": "client",
          "status": "init",
          "params": {
            "dial_option_name": "extphone"
          },
          "triggers": [
            {
              "trigger_tag": "bridge_main",
              "trigger_status": "BridgeCreated",
              "action": "start",
              "active": true
            },
            {
              "trigger_tag": "client",
              "trigger_status": "Dial#BUSY",
              "action": "func",
              "func": "get_sip_and_q850",
              "active": true
            },
            {
              "trigger_tag": "client",
              "trigger_status": "Dial#ANSWER",
