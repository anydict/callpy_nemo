      ],
      "content": [
        {
          "type": "chan_snoop",
          "tag": "snoop_client",
          "status": "init",
          "params": {
            "target_chan_tag": "client"
          },
          "triggers": [
            {
              "trigger_tag": "bridge_snoop_client",
              "trigger_status": "BridgeCreated",
              "action": "start",
              "active": true
